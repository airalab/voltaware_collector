<launch>
  <node name="trader" pkg="voltaware_collector" type="trader_node" output="screen">
    <!-- Address of a lighthouse you work on, airalab mainnet lighthouse by default -->
    <param name="lighthouse" value="0xD40AC7F1e5401e03D00F5aeC1779D8e5Af4CF9f1" />
    <!-- A unique model Qm...., hello_aira model by default -->
    <param name="model" value="QmNTZw9D4vnpo7PZXUL3jHXNPJMEyTz8qEwKesMXaRYyZC" />
    <!-- A token your CPS works with, DAI token by default -->
    <param name="token" value="0x89d24A6b4CcB1B6fAA2625fE562bDD9a23260359" />
    <!-- Ethereum blocks lifetime, ~12-15 sec for one -->
    <param name="order_lifetime" value="100" />
  </node>
</launch>
